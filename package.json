{
  "name": "daga-session-service",
  "version": "1.0.0",
  "description": "A service for session control and permission",
  "main": "index.js",
  "scripts": {
    "start": "node dist/index.js",
    "build": "tsc --build",
    "grunt": "grunt",
    "doc": "grunt typedoc",
    "build-all": "npm run doc && npm run build && npm run start",
    "dev": "tsc --build && nodemon dist/index.js",
    "test": "mocha -r ts-node/register ./test/**/*.spec.ts --timeout 10000 --exit",
    "obfuscate": "npm run build && grunt obfuscator",
    "generate": "tsoa swagger && tsoa routes"
  },
  "keywords": [
    "session",
    "microservice",
    "permmission",
    "role"
  ],
  "author": "vmgabriel, dbgroldan",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "envalid": "^6.0.1",
    "express": "^4.17.1",
    "frunt": "^0.3.3",
    "grunt-contrib-watch": "^1.1.0",
    "grunt-ts": "^6.0.0-beta.22",
    "grunt-tslint": "^5.0.2",
    "grunt-typedoc": "^0.2.4",
    "helmet": "^3.22.0",
    "https": "^1.0.0",
    "morgan": "^1.10.0",
    "swagger-ui-express": "^4.1.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.6",
    "@types/helmet": "0.0.46",
    "@types/node": "^13.13.4",
    "grunt-browserify": "^5.3.0",
    "grunt-cli": "^1.3.2",
    "swagger": "^0.7.5",
    "swagger-editor": "^3.8.1",
    "ts-node": "^8.9.1",
    "typedoc": "^0.17.6",
    "typescript": "^3.8.3"
  }
